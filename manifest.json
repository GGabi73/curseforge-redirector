{
  "manifest_version": 2,
  "name": "Legacy CurseForge Redirect",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "declarativeNetRequest",
    "tabs"
  ],
  "host_permissions": [
    "*://www.curseforge.com/*"
  ]
}
